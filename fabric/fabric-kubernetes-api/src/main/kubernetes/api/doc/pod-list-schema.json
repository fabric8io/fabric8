{
  "$schema": "http://json-schema.org/draft-03/schema",
  "type": "object",
  "required": false,
  "description": "Returns the current list of pods.",
  "properties": {
    "kind": {
      "type": "string",
      "required": false,
      "enum": [
        "PodList"
      ]
    },
    "id": {
      "type": "string",
      "required": false
    },
    "creationTimestamp": {
      "type": "string",
      "required": false
    },
    "selfLink": {
      "type": "string",
      "required": false
    },
    "items": {
      "type": "array",
      "items": {
        "$ref": "pod-schema.json"
      }
    }
  }
}
