{
  "id": "quickStartJavaSimpleController",
  "kind": "ReplicationController",
  "apiVersion": "v1beta1",
  "desiredState": {
    "replicas": 1,
    "replicaSelector": {
      "name": "javaSimple",
      "container": "java",
      "group": "quickstart"
    },
    "podTemplate": {
      "desiredState": {
        "manifest": {
          "version": "v1beta1",
          "id": "quickStartJavaSimpleController",
          "containers": [
            {
              "name": "quickstart-java-simple",
              "image": "${docker.dataImage}",
              "ports": [
                {
                  "containerPort": 8778,
                  "hostPort": 9804
                }
              ]
            }
          ]
        }
      },
      "labels": {
        "name": "javaSimple",
        "container": "java",
        "group": "quickstart"
      }
    }
  },
  "labels": {
    "name": "javaSimple",
    "container": "java",
    "group": "quickstart"
  }
}
