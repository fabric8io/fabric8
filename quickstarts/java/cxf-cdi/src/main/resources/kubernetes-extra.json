  "items": [
    {
      "id": "java-cxf-cdi",
      "apiVersion": "v1beta1",
      "kind": "Service",
      "containerPort": ${http.port},
      "port": "9002",
      "selector": {
        "container": "java",
        "name": "quickstartJavaCxfCdi",
        "group": "quickstarts"
      }
    }
  ]
}
